                "hazardous": {
                    "@id": "uncefact-vocab:applicableHazardousMaterial"
                }
            }
        },

        "Product": {
            "@id": "schema.org:ProductModel",
            "@context": {
                "modelName": {
                    "@id": "schema.org:model"
                }
            }
        },

        "Regulation": {
          "@id": "untp:ConformityCredential#regulation",
          "@context": {
            "@protected": true,
    
            "id": "@id",
            "type": "@type"
